local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Durk|Script Unv Hub V1.5 New UI", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest", IntroText = "Durk Loading"})
local Tab = Window:MakeTab({
	Name = "Client",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddToggle({
	Name = "Anti-AFK",
	Default = false,
	Callback = function(Value)  if Value then
           -- Ativado
           while Value do
               -- Clique na tecla "R" a cada 18 minutos
               wait(18 * 60) -- 18 minutos em segundos
               game:GetService("UserInputService"):KeyPress(Enum.KeyCode.R)
           end
       end
		print(Value)
	end    
})

Tab:AddToggle({
	Name = "Anti Ban Wave",
	Default = false,
	Callback = function(Value)
local timer = 0 print("test")
while true do
    wait(30 * 60) -- wait 30 minutes
    timer = timer + 1
    Toggle:Callback(not Toggle.CurrentValue) -- toggle the value
end
		print(Value)
	end    
})

local Section = Tab:AddSection({
	Name = "Blox Fruits"
})


Tab:AddSlider({
	Name = "WalkSpeed",
	Min = 5,
	Max = 150,
	Default = 16,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Speed",
	Callback = function(Value)              game.Players.LocalPlayer.Character:SetAttribute("SpeedMultiplier", Value)
		print(Value)
	end    
})

Tab:AddSlider({
	Name = "Dash",
	Min = 0,
	Max = 500,
	Default = 5,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Dash Power",
	Callback = function(Value)
 game.Players.LocalPlayer.Character:SetAttribute("DashLength", Value)
		print(Value)
	end    
})

local Tab = Window:MakeTab({
	Name = "Blox Fruits",
	Icon = "rbxassetid://4483362458",
	PremiumOnly = false
})

Tab:AddButton({
	Name = "Redz Hub",
	Callback = function()
      		print("button pressed")                 loadstring(game:HttpGet("https://raw.githubusercontent.com/REDzHUB/BloxFruits/main/redz9999"))()
  	end    
})

Tab:AddButton({
	Name = "W-Azure",
	Callback = function()
      		print("button pressed")              getgenv().ForceUseSilentAimDashModifier = true --Force turn on silent aim , if error then executor problem
getgenv().ForceUseWalkSpeedModifier = true --Force turn on Walk Speed Modifier , if error then executor problem

 loadstring(game:HttpGet("https://api.luarmor.net/files/v3/loaders/3b2169cf53bc6104dabe8e19562e5cc2.lua"))()

  	end    
})

local Tab = Window:MakeTab({
	Name = "Ucn Test",
	Icon = "rbxassetid://4483362458",
	PremiumOnly = false
})

