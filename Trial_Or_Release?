 })

local Section = Tab:CreateSection("Aimbot Player")

local Toggle = Tab:CreateToggle({
   Name = "Aimbot Beta",
   CurrentValue = false,
   Flag = "Toggle1",
   Callback = function(Value)
       if Value then
           -- Ativado
           while Value do
               -- Encontre o humanoid mais próximo ao humanoid local
               local closestHumanoid = nil
               local closestDistance = math.huge
               for _, player in pairs(game.Players:GetPlayers()) do
                   if player ~= game.Players.LocalPlayer and player.Character and player.Character:FindFirstChild("Humanoid") then
                       local distance = (player.Character.Humanoid.RootPart.Position - game.Players.LocalPlayer.Character.Humanoid.RootPart.Position).magnitude
                       if distance < closestDistance then
                           closestHumanoid = player.Character.Humanoid
                           closestDistance = distance
                       end
                   end
               end

               -- Verificar se o humanoid mais próximo está no mesmo time que o humanoid local
               if closestHumanoid and closestHumanoid.Team ~= game.Players.LocalPlayer.Character.Humanoid.Team then
                   -- Verificar se há uma parede na frente do humanoid mais próximo
                   local ray = Ray.new(closestHumanoid.RootPart.Position, (closestHumanoid.RootPart.Position - game.Players.LocalPlayer.Character.Humanoid.RootPart.Position).unit * 5)
                   local hit = workspace:FindPartOnRay(ray)
                   if hit and hit:IsA("BasePart") then
                       -- Há uma parede na frente, não mirar no humanoid
                       wait(0.0006)
                   else
                       -- Mirar no humanoid mais próximo
                       game.Workspace.Camera.CFrame = CFrame.new(game.Workspace.Camera.CFrame.Position, closestHumanoid.RootPart.Position)
                   end
               end
               wait(0.0006)
           end
       end
   end,
})

