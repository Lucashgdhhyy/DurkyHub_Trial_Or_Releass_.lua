local OrionLib = loadstring(game:HttpGet(("https://raw.githubusercontent.com/shlexware/Orion/main/source")))()
local Window = OrionLib:MakeWindow({Name = "Durk | Unv Hub V2.0", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest", IntroText = "Durk Loading"})
local Tab = Window:MakeTab({
	Name = "Credits",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddParagraph("Donos:","Durky,MC_Flaming")
Tab:AddParagraph("Progamadores:","Durky")
Tab:AddParagraph("Beta Testers:","MC_Flaming")

Tab:AddButton({
	Name = "Junte-se a comunidade Durk",
	Callback = function()
      		setclipboard("https://discord.gg/bbntkpr65B")
  	end    
})

Tab:AddButton({
    Name = "Resetar",
    Callback = function()
        local character = game.Players.LocalPlayer.Character
        if character then
            local humanoid = character:FindFirstChild("Humanoid")
            if humanoid then
                humanoid.Health = 0
            end
        end
    end
})

Tab:AddTextbox({
	Name = "Beta Version Text:",
	Default = "Input Key Here",
	TextDisappear = true,
	Callback = function(Value)
		if Value == "Beta_Admin" then
			print("You are an Admin!")
		end
		print(Value)
	end	  
})

