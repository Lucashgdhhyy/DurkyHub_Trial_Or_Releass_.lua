local Player = game.Players.LocalPlayer
local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
local Window = Rayfield:CreateWindow({
   Name = "Durk X 1.3 Key System Removed",
   LoadingTitle = "Thx For use",
   LoadingSubtitle = "by gsuk",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil, -- Create a custom folder for your hub/game
      FileName = "Durk Hub"
   },
   Discord = {
      Enabled = false,
      Invite = "noinvitelink", -- The Discord invite code, do not include discord.gg/. E.g. discord.gg/ABCD would be ABCD
      RememberJoins = true -- Set this to false to make them join the discord every time they load it up
   },
   KeySystem = false, -- Set this to true to use our key system
   KeySettings = {
      Title = "Durk X",
      Subtitle = "Key System",
      Note = "Copy key system from clipboard",
      FileName = "Key", -- It is recommended to use something unique as other scripts using Rayfield may overwrite your key file
      SaveKey = true, -- The user's key will be saved, but if you change the key, they will be unable to use your script
      GrabKeyFromSite = true, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
      Key = {"https://pastebin.com/raw/hiprtnku"} -- List of keys that will be accepted by the system, can be RAW file links (pastebin, github etc) or simple strings ("hello","key22")
   }
})
local Tab = Window:CreateTab("Client", 4483362458) -- Title, Image

local Slider = Tab:CreateSlider({
   Name = "WalkSpeed",
   Range = {16, 750},
   Increment = 16,
   Suffix = "Speed",
   CurrentValue = 10,
   Flag = "Slider1", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
   Callback = function(Value)
       -- Get the local player's character
       local character = game.Players.LocalPlayer.Character
       if character then
           -- Get the Humanoid object
           local humanoid = character:FindFirstChild("Humanoid")
           if humanoid then
               -- Set the walk speed to the slider value
               humanoid.WalkSpeed = Value
           end
       end
   end,
})

local Toggle = Tab:CreateToggle({
   Name = "Anti AFK",
   CurrentValue = false,
   Flag = "Toggle1",
   Callback = function(Value)
       if Value then
           -- Ativado
           while Value do
               -- Clique na tecla "R" a cada 18 minutos
               wait(18 * 60) -- 18 minutos em segundos
               game:GetService("UserInputService"):KeyPress(Enum.KeyCode.R)
           end
       end
       print("")            Rayfield:Notify({
   Title = "Anti-Afk Start",
   Content = "Clicking (R) after 18Minutes",
   Duration = 6.5,
   Image = 4483362458,
   Actions = { -- Notification Buttons
      Ignore = {
         Name = "Okay!",
         Callback = function()
         print("The user tapped Okay!")
      end
   },
},
})
   end,
})

local Toggle = Tab:CreateToggle({
   Name = "Anti-Ban Wave",
   CurrentValue = false,
   Flag = "Toggle1",
   Callback = function(Value)
   local timer = 0 print("test")
while true do
    wait(30 * 60) -- wait 30 minutes
    timer = timer + 1
    Toggle:Callback(not Toggle.CurrentValue) -- toggle the value
end
  --iable (Value) is a boolean on whether the toggle is true or false
   end,
})



local Input = Tab:CreateInput({
   Name = "Fly Keybine [BETA]",
   PlaceholderText = "Input you keybine",
   RemoveTextAfterFocusLost = false,
   Callback = function(Text)
       -- The function that takes place when the input is changed
       -- The variable (Text) is a string for the value in the text box
       local key = Enum.KeyCode[Text:upper()] -- Convert the input text to a KeyCode
       if key then
           game:GetService("UserInputService").InputBegan:Connect(function(input)
               if input.KeyCode == key then
                   -- Ativado
                   local humanoid = game.Players.LocalPlayer.Character.Humanoid
                   humanoid.WalkSpeed = 16
                   humanoid.JumpPower = 50
                   while true do
                       humanoid.Velocity = Vector3.new(0, 0.5, 0) -- Nadar
                       wait()
                   end
               end
           end)
       end
   end,
})

local Section = Tab:CreateSection("Blox Fruits Player")

 local Slider = Tab:CreateSlider({
    Name = "WalkSpeed",
    Range = {16, 150},
    Increment = 1,
    Suffix = "Speed",
    CurrentValue = 35,
    Flag = "Slider1", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
